<table id="datatable-default" class="table display responsive nowrap">
    <thead>
                            
      <tr>
        <th>Date</th>
      <th>Réf</th>
      <th>Date commande</th>
      <th>Client</th>
      <th>Transporteur</th>
      <th>N°BL</th>
      <th>Entrée B3</th>
      <th>Entrée B6</th>
      <th>Entrée B12</th>
      <th>Entrée B32</th>
      <th>Entrée B38</th>
      <th>Sortie B3</th>
      <th>Sortie B6</th>
      <th>Sortie B12</th>
      <th>Sortie B32</th>
      <th>Sortie B38</th>
      <th>Tonnage total</th>
        
        
        
      </tr>
                    
      </thead>
      <tbody >
                     
        <% @orders.each do |order| %>
      <tr>
        <td><%= order.created_at.strftime("%d/%m/%Y") %></td>
        <td><%= order.uid %></td>
        <td><%= order.order_date.strftime("%d/%m/%Y") %></td>
        <td><%= order.customer.full_name %></td>
        <td><%= order.delivery_note_number %></td>
        <td><%= order.transporter %></td>
        
        <td class="order-entry"><%= order.entry_b3 %></td>
        <td class="order-entry"><%= order.entry_b6 %></td>
        <td class="order-entry"><%= order.entry_b12 %></td>
        <td class="order-entry"><%= order.entry_b32 %></td>
        <td class="order-entry"><%= order.entry_b38 %></td>
        
        <td class="order-out"><%= order.out_b3 %></td>
        <td class="order-out"><%= order.out_b6 %></td>
        <td class="order-out"><%= order.out_b12 %></td>
        <td class="order-out"><%= order.out_b32 %></td>
        <td class="order-out"><%= order.out_b38 %></td>
        <td class="order-out"><%= number_to_currency order.total_tonnage , unit: "", seperator: ",", delimiter: " " %></td>
        
      
        
        
      </tr>
    <% end %>

    </tbody>
    <tfoot class="tfooter">
        <%= render("search/partials/orders/tfooter") %> 
      
    </tfoot>
</table>


  <div id="withdrawal-modal" class="c-modal modal fade" data-backdrop="static"></div>
                 