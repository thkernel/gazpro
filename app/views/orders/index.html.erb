    <tr>
      <th>Date</th>
      <th>Réf</th>
      <th>Date commande</th>
      <th>Client</th>
      <th>Transporteur</th>
      <th>N°BL</th>
      <th>Entrée B3</th>
      <th>Entrée B6</th>
      <th>Entrée B12</th>
      <th>Entrée B32</th>
      <th>Entrée B38</th>
      <th>Sortie B3</th>
      <th>Sortie B6</th>
      <th>Sortie B12</th>
      <th>Sortie B32</th>
      <th>Sortie B38</th>
      <th>Tonnage total</th>
      <th>Statut</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody class="orders">
    <%= render 'index' %>
  </tbody>
</table>
</div>
</div>
<div id="order-modal" class="c-modal modal fade" data-backdrop="static"></div>
