<div class="br-pageheader mg-30">
      <nav class="breadcrumb pd-0 mg-0 tx-12">
        
        <span class="breadcrumb-item active"><h5>Liste des retraits de colis</h5></span>
      </nav>
</div><!-- br-pageheader -->
<div class="br-pagebody">
  <div class="br-section-wrapper">
    <div class="headers mg-b-20">
        <div class="left-content">
        </div>
        <% if can? :create, Withdrawal %>
        <div class="text-right">
            <%= link_to raw('<i class="fa fa-plus" aria-hidden="true"></i> Nouveau retrait'), search_sending_parcels_path, class: "btn btn-primary tx-mont tx-medium tx-11 tx-uppercase pd-y-12 pd-x-25 tx-spacing-1", remote: false %>

        </div>
        <% end %>

    </div>


<table id="datatable-default" class="table display responsive nowrap">
  <thead>
    <tr>
      <th>Date</th>
      <th>Réf</th>
      <th>Réf envoi</th>
      <th>Statut</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody class="parcel-collections">
    <%= render "index" %>
  </tbody>
</table>

</div>
</div>
<div id="parcel-collection-modal" class="c-modal modal fade" data-backdrop="static"></div>
